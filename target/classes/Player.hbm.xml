<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.av.fantasy.Player" table="Bball_Players">
<id column="name" name="name" type="java.lang.String"/>
<property column="avg_assists" name="averageAssists" type="java.lang.Double"/>
<property column="avg_blocks" name="averageBlocks" type="java.lang.Double"/>
<property column="avg_points" name="averagePoints" type="java.lang.Double"/>
<property column="avg_rebounds" name="averageRebounds" type="java.lang.Double"/>
<property column="avg_steals" name="averageSteals" type="java.lang.Double"/>
<property column="avg_turnovers" name="averageTurnovers" type="java.lang.Double"/>
<property column="modifier" name="Modifier" type="java.lang.Double"/>
<property column="salary" name="Salary" type="java.lang.Integer"/>
<property column="team" name="team" type="java.lang.String"/>
</class>
</hibernate-mapping>
